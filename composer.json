    "yuncms/yii2-coin": "~1.0.0",
    "yuncms/yii2-doing": "~1.0.0",
    "yuncms/yii2-support": "~1.0.0",
    "yuncms/yii2-attention": "~1.0.0",
    "yuncms/yii2-collection": "~1.0.0",
    "yuncms/yii2-notification": "~1.0.0",
    "yuncms/yii2-authentication": "~1.0.0"
