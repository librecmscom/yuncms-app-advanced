{
    "name": "yuncms/yii2-app-advanced",
    "description": "Yuncms Advanced Project Template",
    "keywords": ["yuncms", "Yii 2", "framework", "advanced", "project template"],
    "homepage": "http://www.yuncms.net/",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yuncms/yuncms-app-advanced/issues?state=open",
        "source": "https://github.com/yuncms/yuncms-app-advanced"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yuncms/yii2-admin": "~2.0",
        "yuncms/yii2-user": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "optimize-autoloader": true,
        "notify-on-install": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
