{
    "name": "yuncms/yii2-app-advanced",
    "description": "Yuncms Advanced Project Template",
    "keywords": ["yuncms", "Yii 2", "framework", "advanced", "project template"],
    "homepage": "http://www.yuncms.net/",
    "type": "project",
    "license": "MIT",
    "support": {
