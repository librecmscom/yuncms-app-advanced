    "xutl/yii2-fontawesome-asset": "~2.0.0",
    "2amigos/qrcode-library": "~1.1",
    "yuncms/yii2-admin": "~2.0",
    "yuncms/yii2-user": "~2.0",
    "yuncms/yii2-attachment": "~2.0",
    "yuncms/yii2-system": "~2.0",
